        XDEF    carro,moto,aviao
        XREF    maria,marta
        ADD     D1,labeT
labeT:  ADD     211,D2
labaT:  ADD     D2,labeT
carro:  CLR     120
moto:   ADD     marta,D1
        STOP    #200
        NEG     labaT
        NOT     labeT
        JUMP    moto
        NOT     200
        BRA     D1
        MOVES   D1,D3
END
